<div class='sign_up'>
  <div class='sign_up_right'>
    <% if @user.errors.any? %>
      <h1> TRY AGAIN! </h1>
      <ol>
        <% @user.errors.full_messages.each do |msg| %>
        <li><%=  msg  %></li>
        <% end %>
      </ol>
    <% end %>
  <%= render :partial => 'form' %>
  </div>
</div>
